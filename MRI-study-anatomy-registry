    <script>
        let loadedCount = 0;
        const totalImages = 106;

        function loadImage(input, previewId) {
            if (input.files && input.files[0]) {
                const reader = new FileReader();
                
                reader.onload = function(e) {
                    const img = document.getElementById(previewId);
                    const placeholder = img.parentElement;
                    
                    // Check if this is a new image (not replacing)
                    const isNewImage = !placeholder.classList.contains('has-image');
                    
                    img.src = e.target.result;
                    placeholder.classList.add('has-image');
                    
                    if (isNewImage) {
                        loadedCount++;
                        updateProgress();
                    }
                };
                
                reader.readAsDataURL(input.files[0]);
            }
        }

        function updateProgress() {
            const percentage = Math.round((loadedCount / totalImages) * 100);
            const progressBar = document.getElementById('progressBar');
            progressBar.style.width = percentage + '%';
            progressBar.textContent = `${loadedCount} of ${totalImages} images loaded (${percentage}%)`;
        }
    </script>
</body>
</html>
